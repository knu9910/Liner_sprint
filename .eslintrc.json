{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": ["eslint:recommended","plugin:prettier/recommended"],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "prettier/prettier": ["error", {
        "endOfLine":"auto"
    }],
    "rules": {
        "no-unused-vars": 0
    }
}
